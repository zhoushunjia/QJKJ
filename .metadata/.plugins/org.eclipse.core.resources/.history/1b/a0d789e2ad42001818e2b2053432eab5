+                if(i == start) {
+                    pagingModel.unshift({
+                        index:1,
+                        class:"first",
+                        text:"首页"
+                    },{
+                        index:data.pageNo-1,
+                        class:"prev",
+                        text:"上一页"
+                    });
+                }
+                // if(isEnd && data.pageNo < data.totalPages) {
+                if(isEnd) {
+                    pagingModel.push({
+                        index:data.pageNo + 1,
+                        class:"next",
+                        text:"下一页"
+                    },{
+                        index:data.totalPages,
+                        class:"last",
+                        text:"末页"
+                    })
+                }
+            }
+            $artlPaging.html(format(temp,pagingModel));
+        },
+        formatTime:function ($elem,timeText,timetype) {
+            timeText = timeText.replace(/\s+(.+)+/g,"");
