+	//创建添加二级导航容器的菜单
+	wqdNavigate.createAddBtn = function(){
+		$(".wqdAddsecondnav").remove();
+		$(".wqdNavMenuWrap").remove();
+		var that = $(this),
+			isSideNav = that.parents(".sectionV2").hasClass("wqdSideNavWrap") || that.parents(".sectionV2").hasClass("wqdPhoneNav"),
