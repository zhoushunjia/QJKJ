+$(function() {
+	var wqdNavigate = {};
+	//侧边导航浮动模式的展开/收起
+	wqdNavigate.commonInit = function(){
+		$(document).on("click",function(event){
+			var that = $(".wqdSideNavWrap.floatStatus"),
+				target = $(event.target);
+			if(!that.length || !that.is(":visible")) return;
