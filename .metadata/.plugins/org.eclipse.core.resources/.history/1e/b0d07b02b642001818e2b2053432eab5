+	    		thatWidth = that.width();
+	    	if(that.is(":visible")) return;
+    		that.css({"width":0,"overflow":"hidden"});
+			that.show().animate({"width":thatWidth},200,"",function(){
+				that.attr("style",that.attr("style").replace(/overflow:(\d|\w|\s|)+;/g,""));
+			});
+	    });
